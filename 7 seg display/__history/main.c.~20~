#include <main.h>

#byte portb=7

void main()
{
   set_tris_a(0x00);
   set_tris_c(0x00);
   setup_psp(psp_disabled);
   setup_ccp1(ccp_off);
   setup_ccp2(ccp_off);
   setup_timer_1(t1_disabled);
   setup_timer_2(t2_disabled,0,1);
   setup_adc_ports(no_analogs);
   setup_adc(adc_off);
  
  output_c(0x00);
  
   while(TRUE)
   {
      output_high(pin_b1);
      delay_ms(100);
      output_low(pin_b1);
      delay_ms(100);
   }

}
